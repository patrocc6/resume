                            <div class="col-sm-6">
                                <div class="result" style="padding-top:12px">
                                    <% if flash[:notice] %>
                                       <div class="notice"><%= flash[:notice] %></div>
                                    <% end %>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
